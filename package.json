{
    "name": "@niu_/vue-tools",
    "version": "1.2.1",
    "description": "vue tools",
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack serve --config build/webpack.dev.js",
        "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.js",
        "build-css-in": "cross-env NODE_ENV=production CSSIN=true webpack --config build/webpack.prod.js",
        "lint": "eslint src --fix --ext .ts,.tsx,.js,.jsx,.vue",
        "test:unit": "jest --config test/unit/jest.config.js --coverage"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/niuxinyu/vue-tools.git"
    },
    "keywords": [
        "vue",
        "tools"
    ],
    "main": "dist/vue-tools.min.js",
    "author": "niu",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/niuxinyu/vue-tools/issues"
    },
    "typings": "types/index.d.ts",
    "files": [
        "dist",
        "types"
    ],
    "homepage": "https://github.com/niuxinyu/vue-tools#readme",
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
        "@babel/plugin-transform-runtime": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-typescript": "^7.12.7",
        "@babel/runtime": "^7.12.5",
        "@babel/runtime-corejs3": "^7.12.5",
        "@typescript-eslint/eslint-plugin": "^4.11.0",
        "@typescript-eslint/parser": "^4.11.0",
        "autoprefixer": "^8.0.0",
        "babel-loader": "^8.2.2",
        "clean-webpack-plugin": "^3.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^5.0.1",
        "eslint": "^7.18.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-vue": "^7.3.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^4.5.0",
        "husky": "^4.3.6",
        "jest": "^26.6.3",
        "less": "^4.0.0",
        "less-loader": "^7.1.0",
        "lint-staged": "^10.5.3",
        "mini-css-extract-plugin": "^1.3.3",
        "optimize-css-assets-webpack-plugin": "^5.0.4",
        "postcss-loader": "^4.1.0",
        "style-loader": "^2.0.0",
        "ts-jest": "^26.4.4",
        "ts-loader": "^8.0.12",
        "typescript": "^4.1.3",
        "url-loader": "^4.1.1",
        "vue-eslint-parser": "^7.4.1",
        "vue-loader": "^15.9.6",
        "vue-template-compiler": "^2.6.12",
        "webpack": "^5.11.0",
        "webpack-cli": "^4.2.0",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^5.7.3"
    },
    "dependencies": {
        "core-js": "^3.8.1",
        "vue": "^2.6.12",
        "vue-class-component": "^7.2.6",
        "vue-property-decorator": "^9.1.2",
        "vue-router": "^3.4.9"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "npm run lint",
            "git add ."
        ]
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
